{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "22.0.0",
  "include": [
    "src/**/*",
    "@types/*.d.ts"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "lib": [
      "es2023"
    ],
    "module": "node16",
    "target": "es2022",
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node16"
  }
}